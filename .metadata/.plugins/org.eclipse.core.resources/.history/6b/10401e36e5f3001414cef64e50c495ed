import java.util.Scanner;
public class Ex03 {

	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		int[]n = new int[10];
		
		int i=0,soma=0;
		double media=0;
		
		while (i<10){
			System.out.println("Insira o "+(i+1)+" número:");
			n[i]=entrada.nextInt();
			i++;
		}
		
		while (i<10){
			soma=soma+n[i];
		}
		
		media=soma/10;
				
		for (i=0;i>10;i++){
			System.out.println("A média é: "+media);
		}
		entrada.close();

	}

}
